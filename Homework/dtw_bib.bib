@article{Viljanen2001,
abstract = {Geomagnetically induced currents (GICs) in technological conductor systems are a manifestation of the ground effects of space weather. Large GICs are always associated with large values of the time derivative of the geomagnetic field, and especially with its horizontal component ({\textless}i{\textgreater}d{\textless}b{\textgreater}H{\textless}/b{\textgreater}/dt{\textless}/i{\textgreater}). By using the IMAGE magnetometer data from northern Europe from 1982 to 2001, we show that large {\textless}i{\textgreater}d{\textless}b{\textgreater}H{\textless}/b{\textgreater}/dt{\textless}/i{\textgreater}'s (exceeding 1 nT/s) primarily occur during events governed by westward ionospheric currents. However, the directional distributions of {\textless}i{\textgreater}d{\textless}b{\textgreater}H{\textless}/b{\textgreater}/dt {\textless}/i{\textgreater}are much more scattered than those of the simultaneous baseline subtracted horizontal variation field vector ?{\textless}b{\textgreater}{\textless}i{\textgreater}H{\textless}/i{\textgreater}{\textless}/b{\textgreater}. A pronounced difference between ?{\textless}b{\textgreater}{\textless}i{\textgreater}H{\textless}/i{\textgreater}{\textless}/b{\textgreater} and {\textless}i{\textgreater}d{\textless}b{\textgreater}H{\textless}/b{\textgreater}/dt{\textless}/i{\textgreater} takes place at about 02?06 MLT in the auroral region when {\textless}i{\textgreater}d{\textless}b{\textgreater}H{\textless}/b{\textgreater}/dt {\textless}/i{\textgreater}prefers an east-west orientation, whereas ?{\textless}b{\textgreater}{\textless}i{\textgreater}H{\textless}/i{\textgreater}{\textless}/b{\textgreater} points to the south. The occurrence of large {\textless}i{\textgreater}d{\textless}b{\textgreater}H{\textless}/b{\textgreater}/dt{\textless}/i{\textgreater} has two daily maxima, one around the local magnetic midnight, and another in the morning. There is a single maximum around the midnight only at the southernmost IMAGE stations. An identical feature is observed when large GICs are considered. The yearly number of large {\textless}i{\textgreater}d{\textless}b{\textgreater}H{\textless}/b{\textgreater}/dt{\textless}/i{\textgreater} values in the auroral region follows quite closely the {\textless}i{\textgreater}aa{\textless}/i{\textgreater} index, but a clear variation from year-to-year is observed in the directional distributions. The scattering of {\textless}i{\textgreater}d{\textless}b{\textgreater}H{\textless}/b{\textgreater}/dt{\textless}/i{\textgreater} distributions is smaller during descending phases of the sunspot cycle. Seasonal variations are also seen, especially in winter {\textless}i{\textgreater}d{\textless}b{\textgreater}H{\textless}/b{\textgreater}/dt {\textless}/i{\textgreater} is more concentrated to the north-south direction than at other times. The results manifest the importance of small-scale structures of ionospheric currents when GICs are considered. The distribution patterns of {\textless}i{\textgreater}d{\textless}b{\textgreater}H{\textless}/b{\textgreater}/dt{\textless}/i{\textgreater} cannot be explained by any simple sheet-type model of (westward) ionospheric currents, but rapidly changing north-south currents and field-aligned currents must play an important role.},
author = {Viljanen, A. and Nevanlinna, H. and Pajunp{\"{a}}{\"{a}}, K. and Pulkkinen, A.},
file = {:Users/dwelling/Library/Application Support/Mendeley Desktop/Downloaded/Viljanen et al. - 2001 - Time derivative of the horizontal geomagnetic field as an activity indicator.pdf:pdf},
journal = {Annales Geophysicae},
mendeley-groups = {GIC Refs},
number = {9},
pages = {1107--1118},
title = {{Time derivative of the horizontal geomagnetic field as an activity indicator}},
volume = {19},
year = {2001}
}

